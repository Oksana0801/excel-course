{
  "parser" : "babel-eslint",
  "env" : {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "arrow-parens": "off",
    "comma-dangle": "off",
    "semi": "off",
    "require-jsdoc": "off",
    "eol-last": "off",
    "no-tabs": "off",
    "no-mixed-spaces-and-tabs": "off",
    "operator-linebreak": "off"
  },
  "plugins": ["jest"],
  "extends": ["eslint:recommended", "google"]
}